    <path d="M168.611 72.185l-3.766 10.006h11.589a7.086 7.086 0 007.101-7.1 7.09 7.09 0 00-4.381-6.566 7.088 7.088 0 00-2.72-.535h-1.771a6.464 6.464 0 00-6.052 4.195z" fill="#FFAB79"/>
    <path d="M174.714 68.007a6.535 6.535 0 00-6.121 4.23l-3.748 9.954h4.711a4.347 4.347 0 004.075-2.82l2.82-7.479h9.645a1.928 1.928 0 001.806-1.272 1.92 1.92 0 00-.907-2.395 1.929 1.929 0 00-.899-.218h-11.382z" fill="#FFAB79"/>
